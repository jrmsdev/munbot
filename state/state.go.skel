package state

import (
	"context"
)

var _ State = &XXXState{}

type XXXState struct {
	m   *sm
	err error
}

func newXXX(m *sm) *XXXState {
	return &XXXState{m: m}
}

func (s *XXXState) String() string {
	return "XXX"
}

func (s *XXXState) Error() error {
	return s.err
}

func (s *XXXState) Run(ctx context.Context) Status {
	select {
	case <-ctx.Done():
		return DONE
	default:
	}
	return EXIT
}
